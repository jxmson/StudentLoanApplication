        <div class="row">
            <h5>THIRD STUDY CHOICE</h5>
            <div class="col-4 col-sm-6 col-md-3">
                <label asp-for="Name3"></label>
                <p>@Model.Name3</p>
            </div>
            <div class="col-4 col-sm-6 col-md-3">
                <label asp-for="Course3"></label>
                <p>@Model.Name3</p>
