@model Application
@{
    ViewData["Title"] = "Application Details";
}

    <div id="ApplicationDetails">
        <h2>Application details</h2>

        <h5>PERSONAL INFORMATION</h5>
        <div class="row">
            <div class="col-4 col-sm-6 col-md-3">
                <label asp-for="Surname"></label>
                <p>@Model.Surname</p>
            </div>
            <div class="col-4 col-sm-6 col-md-3">
                <label asp-for="FirstName"></label>
                <p>@Model.FirstName</p>
            </div>
            <div class="col-4 col-sm-6 col-md-3">
                <label asp-for="OtherName"></label>
                <p>@Model.OtherName</p>
            </div>
            <div class="col-4 col-sm-6 col-md-3">
                <label asp-for="GenderName"></label>
                <p>@Model.GenderName</p>
            </div>
            <div class="col-4 col-sm-6 col-md-3">
                <label asp-for="DOB"></label>
                <p>@Model.DOB</p>
            </div>
            <div class="col-4 col-sm-6 col-md-3">
                <label asp-for="IDNumber"></label>
                <p>@Model.IDNumber</p>
            </div>
            <div class="col-4 col-sm-6 col-md-3">
                <label asp-for="OccupationName"></label>
                <p>@Model.OccupationName</p>
            </div>
            <div class="col-4 col-sm-6 col-md-3">
                <label asp-for="MaritalStatusName"></label>
                <p>@Model.MaritalStatusName</p>
            </div>
            <div class="col-4 col-sm-6 col-md-3">
                <label asp-for="Email"></label>
                <p>@Model.Email</p>
            </div>
            <div class="col-4 col-sm-6 col-md-3">
                <label asp-for="Cell"></label>
                <p>@Model.Cell</p>
            </div>
            <div class="col-4 col-sm-6 col-md-3">
                <label asp-for="Work"></label>
                <p>@Model.Work</p>
            </div>
            <div class="col-4 col-sm-6 col-md-3">
                <label asp-for="Home"></label>
                <p>@Model.Home</p>
            </div>
            <div class="ccol-4 col-sm-6 col-md-3">
                <label asp-for="Address1"></label>
                <p>@Model.Address1</p>
            </div>
            <div class="col-4 col-sm-6 col-md-3">
                <label asp-for="Address2"></label>
                <p>@Model.Address2</p>
            </div>
            <div class="col-4 col-sm-6 col-md-3">
                <label asp-for="Address3"></label>
                <p>@Model.Address3</p>
            </div>
            <div class="col-4 col-sm-6 col-md-3">
                <label asp-for="City"></label>
                <p>@Model.City</p>
            </div>
            <div class="col-4 col-sm-6 col-md-3">
                <label asp-for="Suburb"></label>
                <p>@Model.Suburb</p>
            </div>
            <div class="col-4 col-sm-6 col-md-3">
                <label asp-for="Zip"></label>
                <p>@Model.Zip</p>
            </div>
            <div class="col-4 col-sm-6 col-md-3">
                <label asp-for="Country"></label>
                <p>@Model.Country</p>
            </div>
        </div>
        <div class="row">
            <h5>FIRST STUDY CHOICE</h5>
            <div class="col-4 col-sm-6 col-md-3">
                <label asp-for="Name"></label>
                <p>@Model.Name</p>
            </div>
            <div class=" col-4 col-sm-6 col-md-3">
                <label asp-for="Course"></label>
                <p>@Model.Course</p>
            </div>
            <div class="col-4 col-sm-6 col-md-3">
                <label asp-for="StudentNumber"></label> If applicable
                <p>@Model.StudentNumber</p>
            </div>
            <div class="col-4 col-sm-6 col-md-3">
                <label asp-for="DateofAdmission"></label>
                <p>@Model.DateofAdmission</p>
            </div>
            <div class="col-4 col-sm-6 col-md-3">
                <label asp-for="DateOfCompletion"></label>
                <p>@Model.DateOfCompletion</p>
            </div>
        </div>
        <div class="row">
            <h5>SECOND STUDY CHOICE</h5>
