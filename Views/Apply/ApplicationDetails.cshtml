                <p>@Model.StudentNumber3</p>
            </div>
            <div class="col-4 col-sm-6 col-md-3">
                <label asp-for="DateofAdmission3"></label>
                <p>@Model.DateofAdmission3</p>
            </div>
            <div class="col-4 col-sm-6 col-md-3">
                <label asp-for="DateOfCompletion3"></label>
                <p>@Model.DateOfCompletion3</p>
            </div>
        </div>
        <div class="row">
            <h5>EDUCATIONAL BACKGROUND</h5>
            <div class="col-4 col-sm-6 col-md-3">
                <label asp-for="HighSchool"></label>
                <p>@Model.HighSchool</p>
            </div>
            <div class="col-4 col-sm-6 col-md-3">
                <label asp-for="DateOfSchoolCompletion"></label>
                <p>@Model.DateOfSchoolCompletion</p>
            </div>
            <div class="col-4 col-sm-6 col-md-3">
